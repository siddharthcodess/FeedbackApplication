<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_Scale_Rule</fullName>
    <active>true</active>
    <description>Rating must be from 1 to 5</description>
    <errorConditionFormula>Rating__c &gt; 5  || Rating__c &lt; 1</errorConditionFormula>
    <errorDisplayField>Rating__c</errorDisplayField>
    <errorMessage>Error Message text box, enter Invalid rating. Rating must be from 1 to 5</errorMessage>
</ValidationRule>
